<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="5dp" >   
        

        <TextView
            android:id="@+id/lblRank"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#" />

        <ImageView
            android:id="@+id/imgTopCategory"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingLeft="5dp"
            android:src="@drawable/drinks_off" />
        
		<View 
		    android:id="@+id/venueToken"
		    android:layout_width="15dp"
		    android:layout_height="15dp"  
		    android:background="@drawable/background_circle" 
		    android:layout_marginLeft="8dp"
		    android:layout_marginRight="8dp"
		    android:layout_gravity="center"/>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/lblVenueName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="foobar"
                android:textSize="@dimen/venuename" />

            <LinearLayout
                android:id="@+id/llVenueRankCategories"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lblDrinks"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:text="dr:0" />

                <TextView
                    android:id="@+id/lblMusic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:text="mu:0" />

                <TextView
                    android:id="@+id/lblGirls"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:text="gi:0" />

                <TextView
                    android:id="@+id/lblGuys"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:singleLine="true"
            		android:ellipsize="end"
                    android:text="gu:0" />

            </LinearLayout>
        
        </LinearLayout>

    </LinearLayout>



</LinearLayout>